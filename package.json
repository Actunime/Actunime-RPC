{
  "name": "rpc-custom-by-devlerito",
  "productName": "Actunime RPC",
  "version": "1.0.3",
  "description": "Statut personnalisé pour Discord, il est possible que cette application évolue dans le temps.",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "electron-forge start",
    "build-installer": "electron-builder"
  },
  "build": {
    "asar": true,
    "compression": "maximum",
    "files": [
      "build",
      "index.html"
    ],
    "appId": "fr.actunime.actunime",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/iconx256.ico"
    },
    "nsis": {
      "installerIcon": "build/iconx256.ico",
      "uninstallerIcon": "build/iconx256.ico",
      "uninstallDisplayName": "Actunime RPC",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Actunime",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@types/discord-rpc": "^4.0.3",
    "@types/node-localstorage": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^20.0.3",
    "electron-builder": "^23.1.0",
    "eslint": "^7.32.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@commonify/lowdb": "^3.0.0",
    "axios": "^0.27.2",
    "discord-rpc": "^4.0.1",
    "electron-log": "^4.4.8",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-unhandled": "^4.0.1",
    "jsonfile": "^6.1.0",
    "lowdb": "^3.0.0",
    "node-localstorage": "^2.2.1"
  }
}
